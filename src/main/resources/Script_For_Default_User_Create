// user_rights table configuration

CREATE TABLE user_rights
(
  "user" bigint,
  rights character varying,
  CONSTRAINT users_rights_users_fkey FOREIGN KEY ("user")
      REFERENCES users (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION
)

INSERT INTO user_rights(
            id, rights)
